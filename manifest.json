{
  "manifest_version": 3,
  "name": "My Extension",
  "short_name": "My Extension",
  "description": "My Personal Chrome Extension",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus", 
    "tabs"],
  "icons": {
    "16": "assets/search.png"
  },
  "background": {"service_worker": "main.js"},
  "commands":{
    "open_proto" :{
      "suggested_key" :{
        "default":"Alt+Z",
        "mac":"Alt+Z"
      },
      "description":"Open Proto View"
    },
    "open_offer_view" :{
      "suggested_key" :{
        "default":"Alt+E",
        "mac":"Alt+E"
      },
      "description":"Open Offer View"
    },
    "open_voucher_view":{
      "suggested_key":{
        "default":"Alt+V",
        "mac":"Alt+V"  
      },
      "description":"Open Voucher View"
    }
  }
}
